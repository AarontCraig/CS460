@model IList<Homework7.Models.Gif>

@{
    ViewBag.Title = "Home Page";
}

<script src="~/Scripts/gifRequest.js"></script>

<div class="jumbotron text-center">
    <h1>Search for GIF's!!!</h1>
</div>

<div class="container text-center">
    <form>
        <div class="input-group">
            <input type="text" class="form-control" placeholder="Search for something fun..." name="search">
            <div class="input-group-btn">
                <button class="btn btn-default" onclick="clickedMe"><i class="glyphicon glyphicon-search"></i></button>
            </div>
        </div>
    </form>
</div>
@foreach (var item in Model)
{
    <img src="@item.url" width="200" height="150" />
}